apiVersion: v1
kind: Service
metadata:
  name: picoclaw-gateway
  namespace: {{ .Namespace }}
  labels:
    app: picoclaw-gateway
    picoclaw.io/tenant: {{ .TenantID }}
spec:
  type: ClusterIP
  selector:
    app: picoclaw-gateway
  ports:
    - port: 18790
      targetPort: 18790
      protocol: TCP
      name: http
